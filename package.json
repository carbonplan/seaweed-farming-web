{
  "name": "article-template",
  "version": "1.0.0",
  "description": "template for developing a standalone carbonplan research article",
  "main": "index.js",
  "scripts": {
    "dev": "next -p 5001",
    "build": "next build",
    "start": "next start",
    "format": "npm run format-js && npm run format-md",
    "format-js": "prettier --write '*.js' '{components,pages,article}/**/*.js'",
    "format-md": "prettier --write README.md 'article/**/*.md' --parser mdx"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/carbonplan/article-template.git"
  },
  "author": "carbonplan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/carbonplan/article-template/issues"
  },
  "homepage": "https://github.com/carbonplan/article-template#readme",
  "dependencies": {
    "@carbonplan/colormaps": "^2.2.0",
    "@carbonplan/components": "^11.1.2",
    "@carbonplan/icons": "^1.1.0",
    "@carbonplan/maps": "2.0.0-develop.4",
    "@carbonplan/theme": "^7.0.0",
    "@mdx-js/loader": "^1.6.22",
    "@next/mdx": "^12.0.7",
    "@theme-ui/color": "^0.13.1",
    "@theme-ui/match-media": "^0.13.1",
    "d3-format": "^2.0.0",
    "ndarray": "^1.0.19",
    "next": "^12.0.7",
    "polished": "^4.1.3",
    "react": "^17.0.2",
    "react-animate-height": "^2.0.23",
    "react-dom": "^17.0.2",
    "theme-ui": "^0.13.1",
    "trim": ">=0.0.3",
    "zarr-js": "^2.1.3"
  },
  "devDependencies": {
    "prettier": "2.2.1"
  }
}
